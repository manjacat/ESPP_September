@model IEnumerable<eSPP.Models.PergerakanGajiModels>
<div id="content">

    <div class="row">
        <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
            <h1 class="page-title txt-color-blueDark">
                <i class="fa-fw fa fa-pencil-square-o"></i>
                Forms
                <span>
                    >
                    Pergerakan Gaji
                </span>
            </h1>
        </div>
    </div>

    <section id="widget-grid" class="">

        <!-- row -->
        <div class="row">

            <!-- NEW WIDGET START -->
            <!-- WIDGET END -->

        </div>

        <!-- end row -->
        <!-- row -->
        <div class="row">

            <!-- NEW WIDGET START -->
            <article class="col-sm-12">

                <!-- Widget ID (each widget will need unique ID)-->
                <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">
                    <!-- widget options:
                    usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">

                    data-widget-colorbutton="false"
                    data-widget-editbutton="false"
                    data-widget-togglebutton="false"
                    data-widget-deletebutton="false"
                    data-widget-fullscreenbutton="false"
                    data-widget-custombutton="false"
                    data-widget-collapsed="true"
                    data-widget-sortable="false"

                    -->
                    <header>
                        <span class="widget-icon"> <i class="fa fa-align-justify"></i> </span>
                        <h2>Pergerakan Gaji</h2>

                    </header>

                    <!-- widget div-->
                    <div>

                        <!-- widget edit box -->
                        <div class="jarviswidget-editbox">
                            <!-- This area used as dropdown edit box -->

                        </div>
                        <!-- end widget edit box -->
                        <!-- widget content -->
                        <div class="widget-body">
                            @if(ViewBag.pengesahan != "1")
                            {
                            <div class="row">
                                <div class="col col-lg-8 col-md-10 col-sm-12 col-xs-12 col-lg-offset-2 col-md-offset-1">
                                    <div class="well">
                                        @using (Html.BeginForm("PergerakanGaji", "Kewangan8", FormMethod.Get))
                                        {

                                            <div class="row">
                                                <div class="col smart-form col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                    <div class="select">
                                                        <select name="key" id="key" class="form-control" disabled ="disabled">
                                                            <option selected disabled="disabled">Jenis</option>
                                                            <option value="1" @((string)ViewBag.key == "1" ? "selected" : "")>No. Pekerja</option>
                                                            <option value="2" @((string)ViewBag.key == "2" ? "selected" : "")>Nama Pekerja</option>
                                                            <option value="3" @((string)ViewBag.key == "3" ? "selected" : "")>No. KP(Baru)</option>
                                                            <option value="4" @((string)ViewBag.key == "4" || (string)ViewBag.key == null ? "selected" : "")>Bulan Pergerakan</option>
                                                        </select>
                                                        <i></i>
                                                    </div>
                                                    @Html.Hidden("key", "4")
                                                </div>
                                                <div class="col smart-form col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                    <div class="select" id="actionText">
                                                        <select class="form-control" id="bulan" name="bulan">
                                                            <option value="" disabled="disabled" selected>Bulan</option>
                                                            <option value="1" @((int?)ViewBag.bulan == 1 ? "selected" : "")>Januari</option>
                                                            <option value="2" @((int?)ViewBag.bulan == 2 ? "selected" : "")>Febuari</option>
                                                            <option value="3" @((int?)ViewBag.bulan == 3 ? "selected" : "")>Mac</option>
                                                            <option value="4" @((int?)ViewBag.bulan == 4 ? "selected" : "")>April</option>
                                                            <option value="5" @((int?)ViewBag.bulan == 5 ? "selected" : "")>Mei</option>
                                                            <option value="6" @((int?)ViewBag.bulan == 6 ? "selected" : "")>Jun</option>
                                                            <option value="7" @((int?)ViewBag.bulan == 7 ? "selected" : "")>Julai</option>
                                                            <option value="8" @((int?)ViewBag.bulan == 8 ? "selected" : "")>Ogos</option>
                                                            <option value="9" @((int?)ViewBag.bulan == 9 ? "selected" : "")>September</option>
                                                            <option value="10" @((int?)ViewBag.bulan == 10 ? "selected" : "")>Oktober</option>
                                                            <option value="11" @((int?)ViewBag.bulan == 11 ? "selected" : "")>November</option>
                                                            <option value="12" @((int?)ViewBag.bulan == 12 ? "selected" : "")>Disember</option>
                                                        </select><i></i>
                                                    </div>
                                                </div>
                                                <div class="col col-lg-2 col-md-2 col-sm-2 col-xs-12">
                                                    <div class="button">
                                                        <button class="btn btn-default btn-primary" type="submit">
                                                            <i class="fa fa-search"></i> Cari
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                            }
                            <br />
                            @using (Html.BeginForm("PergerakanGaji", "Kewangan8", FormMethod.Post, new { id = "form-tambah" }))
                            {
                                //List<HR_MAKLUMAT_PERIBADI> sPekerja = (List<HR_MAKLUMAT_PERIBADI>)ViewBag.sPekerja;
                                List<HR_MAKLUMAT_PERIBADI> sPegawai = (List<HR_MAKLUMAT_PERIBADI>)ViewBag.sPegawai;
                                List<HR_MAKLUMAT_PERIBADI> sPeribadi = (List<HR_MAKLUMAT_PERIBADI>)ViewBag.peribadi;
                                List<HR_GAMBAR_PENGGUNA> gambarUser = (List<HR_GAMBAR_PENGGUNA>)ViewBag.gambar;
                                @Html.AntiForgeryToken()
                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                @Html.Hidden("key2", (string)ViewBag.key)
                                @Html.Hidden("value2", (string)ViewBag.value)
                                @Html.Hidden("bulan2", (int?)ViewBag.bulan)
                                if (Model.Count() > 0)
                                {
                                    var style = "";
                                    if (Model.Count() > 2)
                                    {
                                        style = "overflow-y:scroll; height:840px";
                                    }
                                    if (Model.Count() > 0)
                                    {
                                        <div class="well">
                                            <fieldset>
                                                <div class="row">
                                                    <section>
                                                        <div class="col col-lg-4 col-md-4 col-sm-4 col-xs-12 smart-form">
                                                            <div class="input col-lg-6 col-md-6 col-sm-8 col-xs-8">
                                                                <i class="icon-append fa fa-search"></i>
                                                                <input type="text" name="cariGlobal" id="cariGlobal" placeholder="Cari No. Pekerja" class="form-control" value="" />
                                                            </div>
                                                        </div>
                                                    </section>
                                                    <section>
                                                        <div class="col col-lg-4 col-md-4 col-sm-4 col-xs-12 smart-form">
                                                            <div class="inline-group">
                                                                <label class="radio">
                                                                    <input type="radio" name="jenisPergerakan" value="D" checked="checked">
                                                                    <i></i>Mendatar
                                                                </label>
                                                                <label class="radio">
                                                                    <input type="radio" name="jenisPergerakan" value="S">
                                                                    <i></i>Statik
                                                                </label>
                                                                <label class="radio">
                                                                    <input type="radio" name="jenisPergerakan" value="J">
                                                                    <i></i>Anjakan
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </section>
                                                    <section>
                                                        <div class="col col-lg-4 col-md-4 col-sm-4 col-xs-12 smart-form">
                                                            <label class="checkbox">
                                                                <input type="checkbox" name="pilihPekerja" checked="checked">
                                                                <i></i>Pilih Semua Pekerja ( Jumlah:<span id="jumPilih">@ViewBag.countKew8</span> )
                                                            </label>
                                                        </div>
                                                    </section>
                                                </div>
                                            </fieldset>
                                        </div>
                                    }

                                    <div id="kepala" style="@style">
                                        @for(var item = 0; item < Model.Count(); item++)
                                        {
                                            HR_MAKLUMAT_PERIBADI peribadi = sPeribadi.SingleOrDefault(s => s.HR_NO_PEKERJA == Model.ElementAt(item).HR_NO_PEKERJA);
                                            <div id="parentView_@item" class="parentView">
                                                <div class="well">
                                                    <fieldset>
                                                        <div class="row" id="infoKakitangan">
                                                            <div class="col col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                                                <div class="row">
                                                                    <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                        @{ HR_GAMBAR_PENGGUNA Gambar = gambarUser.SingleOrDefault(s => s.HR_NO_PEKERJA == Model.ElementAt(item).HR_NO_PEKERJA);}

                                                                        @if (Gambar == null)
                                                                        {

                                                                            if (peribadi.HR_JANTINA == "P")
                                                                            {
                                                                                <img id="img" src="~/content/img/avatars/female.png" alt="" width="120" height="120" />
                                                                            }
                                                                            else
                                                                            {
                                                                                <img id="img" src="~/content/img/avatars/male.png" alt="" width="120" height="120" />
                                                                            }

                                                                        }
                                                                        else
                                                                        {
                                                                            var gambar = Gambar.HR_PHOTO + Gambar.HR_FORMAT_TYPE;
                                                                            <img id="img" src="~/content/uploads/@gambar" alt="" width="120" height="120" />
                                                                        }

                                                                    </div>
                                                                </div>

                                                            </div>
                                                            <div class="col col-lg-8 col-md-8 col-sm-10 col-xs-12 smart-form">
                                                                <div class="row">
                                                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                        <label class="label col-lg-2 col-md-2 col-sm-2">Nama Pekerja</label>
                                                                        <div class="col-lg-6 col-md-8 col-sm-8">
                                                                            <label class="input">
                                                                                <i class="icon-append fa fa-user"></i>
                                                                                @Html.TextBox("[" + item + "].HR_NAMA_PEKERJA", peribadi.HR_NAMA_PEKERJA, new { @class = "form-control", disabled = "disabled" })
                                                                            </label>
                                                                        </div>

                                                                    </section>
                                                                </div>
                                                                <div class="row">
                                                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                        <label class="label col-lg-2 col-md-2  col-sm-2">No. KP (Baru)</label>
                                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                                            <label class="input">
                                                                                <i class="icon-append fa fa-list-alt"></i>
                                                                                @Html.TextBox("[" + item + "].HR_NO_KPBARU", peribadi.HR_NO_KPBARU, new { @class = "form-control", disabled = "disabled", maxlength = "12" })
                                                                            </label>
                                                                        </div>

                                                                    </section>
                                                                </div>
                                                                <div class="row">
                                                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                        <label class="label col-lg-2 col-md-2 col-sm-2">No. Pekerja</label>
                                                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                                                            <label class="input">
                                                                                <i class="icon-append fa fa-list-alt"></i>
                                                                                @Html.TextBox("[" + item + "].noPekerja", Model.ElementAt(item).HR_NO_PEKERJA, new { @class = "form-control noPekerja", disabled = "disabled", maxlength = "5" })
                                                                            </label>
                                                                        </div>

                                                                    </section>
                                                                </div>
                                                                <div id="formpergerakangaji">
                                                                    <div class="row">
                                                                        <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                            <label class="label col-lg-2 col-md-2 col-sm-2">Jenis Pergerakan Gaji</label>
                                                                            <div class="col-lg-6 col-md-8 col-sm-8">
                                                                                @if (Model.ElementAt(item).CHECKED == 1)
                                                                                {
                                                                                    <div class="inline-group">
                                                                                        <label class="radio">
                                                                                            @Html.RadioButton("[" + item + "].HR_JENIS_PERGERAKAN", "D", isChecked: true, htmlAttributes: new { @class = "form-control HR_JENIS_PERGERAKAN" })
                                                                                            <i></i>Mendatar
                                                                                        </label>
                                                                                        <label class="radio">
                                                                                            @Html.RadioButton("[" + item + "].HR_JENIS_PERGERAKAN", "S", htmlAttributes: new { @class = "form-control HR_JENIS_PERGERAKAN" })
                                                                                            <i></i>Statik
                                                                                        </label>
                                                                                        <label class="radio">
                                                                                            @Html.RadioButton("[" + item + "].HR_JENIS_PERGERAKAN", "J", htmlAttributes: new { @class = "form-control HR_JENIS_PERGERAKAN" })
                                                                                            <i></i>Anjakan
                                                                                        </label>
                                                                                    </div>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <div class="inline-group">
                                                                                        <label class="radio">
                                                                                            @Html.RadioButton("[" + item + "].HR_JENIS_PERGERAKAN", "D", isChecked: true, htmlAttributes: new { @class = "form-control HR_JENIS_PERGERAKAN", disabled = "disabled" })
                                                                                            <i></i>Mendatar
                                                                                        </label>
                                                                                        <label class="radio">
                                                                                            @Html.RadioButton("[" + item + "].HR_JENIS_PERGERAKAN", "S", htmlAttributes: new { @class = "form-control HR_JENIS_PERGERAKAN", disabled = "disabled" })
                                                                                            <i></i>Statik
                                                                                        </label>
                                                                                        <label class="radio">
                                                                                            @Html.RadioButton("[" + item + "].HR_JENIS_PERGERAKAN", "J", htmlAttributes: new { @class = "form-control HR_JENIS_PERGERAKAN", disabled = "disabled" })
                                                                                            <i></i>Anjakan
                                                                                        </label>
                                                                                    </div>
                                                                                }

                                                                            </div>

                                                                        </section>
                                                                    </div>
                                                                    <div class="row">
                                                                        <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                            <label class="label col-lg-2 col-md-2 col-sm-2">Gred</label>
                                                                            <div class="col-lg-2 col-md-2 col-sm-2">
                                                                                <label class="input">
                                                                                    @Html.TextBox("[" + item + "].HR_GRED", Model.ElementAt(item).HR_GRED, new { @class = "form-control HR_GRED", list = "list-gred", disabled = "disabled" })
                                                                                    <datalist id="list-gred">
                                                                                        @foreach (GE_PARAMTABLE gred in ViewBag.gred)
                                                                                        {
                                                                                            <option value="@gred.SHORT_DESCRIPTION" data-value="@gred.ORDINAL">@gred.ORDINAL</option>
                                                                                        }
                                                                                    </datalist>
                                                                                    @Html.Hidden("[" + item + "].HR_GRED", Model.ElementAt(item).HR_GRED, new { @class = "form-control HR_GRED2"})
                                                                                </label>
                                                                            </div>
                                                                        </section>
                                                                    </div>
                                                                    <div class="row">
                                                                        <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">

                                                                            <section class="col col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                                                <div class="row">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-4">Gaji Min</label>
                                                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                                                        <label class="input">
                                                                                            @Html.TextBox("[" + item + "].HR_GAJI_MIN", Model.ElementAt(item).HR_GAJI_MIN, new { @class = "form-control HR_GAJI_MIN", list = "list-min", disabled = "disabled" })
                                                                                            <datalist id="list-min"></datalist>

                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </section>
                                                                            <section class="col col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                                                <div class="row">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-4">Gaji Max</label>
                                                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                                                        <label class="input">
                                                                                            @Html.TextBox("[" + item + "].HR_GAJI_MAX", Model.ElementAt(item).HR_GAJI_MAX, new { @class = "form-control HR_GAJI_MAX", list = "list-max", disabled = "disabled" })
                                                                                            <datalist id="list-max"></datalist>

                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </section>

                                                                        </div>
                                                                    </div>
                                                                    <div class="row">
                                                                        <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">

                                                                            <section class="col col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                                                <div class="row">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-4">Pergerakan Gaji Pokok</label>
                                                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                                                        <label class="input">
                                                                                            @Html.TextBox("[" + item + "].HR_GAJI_BARU", Model.ElementAt(item).HR_GAJI_BARU, new { @class = "form-control HR_GAJI_BARU", list = "list-gaji", disabled = "disabled" })
                                                                                            <datalist id="list-gaji"></datalist>

                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </section>
                                                                            <section class="col col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                                                <div class="row">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-4">Perubahan Gaji Pokok</label>
                                                                                    <div class="col-lg-4 col-md-4 col-sm-4">
                                                                                        <label class="input">
                                                                                            @Html.TextBox("[" + item + "].HR_JUMLAH_PERUBAHAN", Model.ElementAt(item).HR_JUMLAH_PERUBAHAN, new { @class = "form-control HR_JUMLAH_PERUBAHAN", disabled = "disabled" })
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </section>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                @if (Model.ElementAt(item).CHECKED != 1)
                                                                {
                                                                    <div class="row">
                                                                        <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                            <span style="color:red">Perhatian: Pekerja ini telah membuat pergerakan gaji</span>
                                                                        </section>
                                                                    </div>
                                                                }
                                                            </div>
                                                            <div class="col col-lg-2 col-md-2 col-sm-4 col-xs-2" id="lihat-info">
                                                                <div class="row text-right">
                                                                    <section>
                                                                        <div class="col col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                                            <a class="btn btn-default btn-success lihatInfo" id="lihatInfo_@item" data-id="@Model.ElementAt(item).HR_NO_PEKERJA" title="Lihat Info">
                                                                                Lihat Info
                                                                            </a>
                                                                        </div>
                                                                    </section>
                                                                    <section>
                                                                        <div class="col col-lg-6 col-md-6 col-sm-6 col-xs-12 smart-form">
                                                                            <label class="checkbox">
                                                                                @if (Model.ElementAt(item).CHECKED == 1)
                                                                                {
                                                                                    <input type="checkbox" name="[@item].HR_NO_PEKERJA" class="form-control HR_NO_PEKERJA" id="@(item)__HR_NO_PEKERJA" value="@Model.ElementAt(item).HR_NO_PEKERJA" checked="checked">
                                                                                }
                                                                                else
                                                                                {
                                                                                    <input type="checkbox" name="[@item].HR_NO_PEKERJA" class="form-control HR_NO_PEKERJA" id="@(item)__HR_NO_PEKERJA" value="@Model.ElementAt(item).HR_NO_PEKERJA" disabled="disabled">
                                                                                }

                                                                                <i></i>Pilih Pekerja
                                                                            </label>
                                                                        </div>

                                                                    </section>


                                                                </div>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                                <div id="showTable">

                                                </div>
                                            </div>
                                        }
                                    </div>
                                
                                    <div class="modal fade" id="Modal" tabindex="-1" role="dialog">
                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div id='ModalContent'>
                                                    <div class="modal-header">
                                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                                            &times;
                                                        </button>
                                                        <h4 class="modal-title">
                                                            Tambah Pergerakan Gaji <span class="error"></span>
                                                        </h4>
                                                    </div>
                                                    <div class="modal-body no-padding smart-form">
                                                        <fieldset>
                                                            <div class="row">
                                                                <div class="col col-12">
                                                                    <section>
                                                                        <div class="row">
                                                                            <label class="label col col-4">No. Surat Kebenaran</label>
                                                                            <div class="col col-8">
                                                                                <label class="input">
                                                                                    @Html.TextBox("HR_NO_SURAT_KEBENARAN", null, new { @class = "form-control" })
                                                                                </label>
                                                                            </div>
                                                                        </div>
                                                                    </section>

                                                                    <section>
                                                                        <div class="row">
                                                                            <label class="label col col-4">Butir-Butir Perubahan</label>
                                                                            <div class="col col-8">
                                                                                <label class="textarea textarea-resizable">
                                                                                    @Html.TextArea("HR_BUTIR_PERUBAHAN", null, new { @class = "form -control custom-scroll", rows = "5" })
                                                                                </label>
                                                                            </div>
                                                                        </div>
                                                                    </section>

                                                                    <section>
                                                                        <div class="row">
                                                                            <label class="label col col-4">Nama Pegawai Pengesahan</label>
                                                                            <div class="col col-8">
                                                                                <label class="input">
                                                                                    @Html.TextBox("HR_NAMA_PEGAWAI", null, new { @class = "form-control", list = "list-pegawai", autocomplete = "off" } )
                                                                                    <datalist id="list-pegawai">
                                                                                        @foreach (var pegawai in sPegawai)
                                                                                        {
                                                                                            <option value="@(pegawai.HR_NAMA_PEKERJA) - @(pegawai.HR_NO_PEKERJA)" data-value="@pegawai.HR_NO_PEKERJA"></option>
                                                                                        }
                                                                                    </datalist>
                                                                                    
                                                                                </label>
                                                                                <label class="hide">
                                                                                    @Html.TextBox("HR_NP_FINALISED_HR", null, new { @class = "form-control" })
                                                                                </label>
                                                                            </div>
                                                                        </div>
                                                                    </section>

                                                                </div>
                                                            </div>

                                                        </fieldset>

                                                        <footer>
                                                            <button type="submit" id="btn" class="btn btn-primary">
                                                                <span class="fa fa-save"></span> Simpan
                                                            </button>
                                                            <button type="reset" class="btn btn-default">
                                                                Reset
                                                            </button>
                                                            <button type="button" class="btn btn-default" data-dismiss="modal">
                                                                Kembali
                                                            </button>
                                                        </footer>


                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    }
                                    if (Model.Count() > 0)
                                    {
                                            <footer>
                                                <div class="well text-right">
                                                    @if (ViewBag.pengesahan == "1")
                                                    {
                                                        <button type="button" id="btn-tambah" class="btn btn-primary">
                                                            <i class="fa fa-edit"> Pengesahan</i>
                                                        </button>
                                                    }
                                                    else
                                                    {
                                                        if (ViewBag.countKew8 > 0)
                                                        {
                                                            <button type="button" id="btn-tambah" class="btn btn-primary">
