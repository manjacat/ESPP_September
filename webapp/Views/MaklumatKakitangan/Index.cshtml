@model eSPP.Models.MaklumatKakitanganModels
			<!-- MAIN CONTENT -->
<div id="content">

    <div class="row">
        <div class="col-xs-12 col-sm-7 col-md-7 col-lg-4">
            <h1 class="page-title txt-color-blueDark">
                <i class="fa fa-table fa-fw "></i>
                Maklumat Kakitangan
                <span>
                    >
                    Cari Maklumat Kakitangan
                </span>
            </h1>
        </div>
    </div>

    <!-- widget grid -->
    <section id="widget-grid" class="">
        <!-- row -->
        <div class="row">

            <!-- NEW WIDGET START -->
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">

                <!-- Widget ID (each widget will need unique ID)-->
                <div class="jarviswidget" id="wid-id-3" data-widget-editbutton="false">
                    <!-- widget options:
                    usage: <div class="jarviswidget jarviswidget-color-blueDark" id="wid-id-0" data-widget-editbutton="false">

                    data-widget-colorbutton="false"
                    data-widget-editbutton="false"
                    data-widget-togglebutton="false"
                    data-widget-deletebutton="false"
                    data-widget-fullscreenbutton="false"
                    data-widget-custombutton="false"
                    data-widget-collapsed="true"
                    data-widget-sortable="false"

                    -->
                    <header>
                        <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                        <h2>Cari Maklumat Kakitangan</h2>

                    </header>

                    <!-- widget div-->
                    <div>

                        <!-- widget edit box -->
                        <div class="jarviswidget-editbox">
                            <!-- This area used as dropdown edit box -->

                        </div>
                        <!-- end widget edit box -->
                        <!-- widget content -->
                        <div class="widget-body">
                            
                            <div class="row">

                                <div class="col col-lg-8 col-md-10 col-sm-12 col-xs-12 col-lg-offset-2 col-md-offset-1">
                                    <div class="well">
                                        @using (Html.BeginForm("Index", "MaklumatKakitangan", FormMethod.Get))
                                        {
                                            
                                            <div class="row">
                                                <div class="col smart-form col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                    <div class="select">
                                                        <select name="key" id="key" class="form-control">
                                                            <option selected disabled="disabled">Jenis</option>
                                                            <option value="1">No. Pekerja</option>
                                                            <option value="2">Nama Pekerja</option>
                                                            <option value="3">No. KP(Baru)</option>
                                                        </select>
                                                        <i></i>
                                                    </div>
                                                </div>
                                                <div class="col smart-form col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                    <div class="input">
                                                        <input type="text" name="value" id="value" class="form-control" placeholder="Data" />
                                                    </div>
                                                </div>
                                                <div class="col col-lg-2 col-md-2 col-sm-2 col-xs-12">
                                                    <div class="button">
                                                        <button class="btn btn-default btn-primary" type="submit">
                                                            <i class="fa fa-search"></i> Cari
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                                
                            </div>
                            <br />

                                @using (Html.BeginForm("Index", "MaklumatKakitangan", FormMethod.Post, new { id = "mKakitangan", enctype = "multipart/form-data" }))
                                {
                                    @Html.AntiForgeryToken()
                                    @Html.ValidationBootstrap()
                                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                    if (Model.HR_SENARAI_PERIBADI != null)
                                    {
                                        var style = "";
                                        if(Model.HR_SENARAI_PERIBADI.Count > 4)
                                        {
                                            style = "overflow-y:scroll; height:750px";
                                        }
                                        <div style="@style">
                                            @foreach (var HR_MAKLUMAT_PERIBADI in Model.HR_SENARAI_PERIBADI)
                                            {
                                                <div class="well">
                                                    <fieldset>
                                                        <div class="row" id="infoKakitangan">
                                                            <div class="col col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                                                <div class="row">
                                                                    <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                        @if (Model.HR_GAMBAR_PENGGUNA.HR_NO_PEKERJA == null)
                                                                        {

                                                                            if (Model.HR_MAKLUMAT_PERIBADI.HR_JANTINA == "P")
                                                                            {
                                                                                <img id="img" src="~/content/img/avatars/female.png" alt="" width="120" height="120" />
                                                                            }
                                                                            else
                                                                            {
                                                                                <img id="img" src="~/content/img/avatars/male.png" alt="" width="120" height="120" />
                                                                            }

                                                                        }
                                                                        else
                                                                        {
                                                                            var gambar = Model.HR_GAMBAR_PENGGUNA.HR_PHOTO + Model.HR_GAMBAR_PENGGUNA.HR_FORMAT_TYPE;
                                                                            if (ViewBag.photo == "")
                                                                            {
                                                                                <img id="img" src="~/content/uploads/@gambar" alt="" width="120" height="120" />
                                                                            }
                                                                            else
                                                                            {
                                                                                <img id="img" src="~/content/img/avatars/male.png" alt="" width="120" height="120" />
                                                                            }
                                                                        }

                                                                    </div>
                                                                    @if (ViewBag.key == "4")
                                                                    {
                                                                        <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                            <button type="button" name="upload-image" id="upload-image" class="btn btn-default" style="width:120px" disabled="disabled">Tukar Gambar</button>
                                                                            <input type="file" name="file" id="file" style="display:none" />
                                                                        </div>
                                                                    }

                                                                </div>

                                                            </div>
                                                            <div class="col col-lg-8 col-md-8 col-sm-10 col-xs-12 smart-form">
                                                                <div class="row">
                                                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                        <label class="label col-lg-2 col-md-2 col-sm-2">Nama Pekerja</label>
                                                                        <div class="col-lg-6 col-md-8 col-sm-8">
                                                                            <label class="input">
                                                                                <i class="icon-append fa fa-user"></i>
                                                                                @Html.EditorFor(model => HR_MAKLUMAT_PERIBADI.HR_NAMA_PEKERJA, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })

                                                                            </label>
                                                                        </div>

                                                                    </section>
                                                                </div>
                                                                <div class="row">
                                                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                        <label class="label col-lg-2 col-md-2  col-sm-2">No. KP (Baru)</label>
                                                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                                                            <label class="input">
                                                                                <i class="icon-append fa fa-list-alt"></i>
                                                                                @Html.EditorFor(model => HR_MAKLUMAT_PERIBADI.HR_NO_KPBARU, new { htmlAttributes = new { @class = "form-control", disabled = "disabled", maxlength = "12" } })
                                                                            </label>
                                                                        </div>

                                                                    </section>
                                                                </div>
                                                                <div class="row">
                                                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                        <label class="label col-lg-2 col-md-2 col-sm-2">No. Pekerja</label>
                                                                        <div class="col-lg-2 col-md-2 col-sm-2">
                                                                            <label class="input">
                                                                                <i class="icon-append fa fa-list-alt"></i>
                                                                                @Html.EditorFor(model => HR_MAKLUMAT_PERIBADI.HR_NO_PEKERJA, new { htmlAttributes = new { @class = "form-control", disabled = "disabled", maxlength = "5" } })

                                                                            </label>
                                                                        </div>

                                                                    </section>
                                                                </div>
                                                            </div>
                                                            @if (ViewBag.key != "4")
                                                            {
                                                                <div class="col col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                                                    <section>
                                                                        <a class="btn btn-default btn-primary" href="@Url.Action("Index","MaklumatKakitangan", new { key = "4", value = HR_MAKLUMAT_PERIBADI.HR_NO_PEKERJA})" title="Profil Kakitangan">
                                                                            Lihat Info
                                                                        </a>
                                                                    </section>
                                                                </div>
                                                            }
                                                            @if (ViewBag.key == "4")
                                                            {
                                                                <br />
                                                                <br />
                                                                <br />
                                                                <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12 smart-form">
                                                                    <div class="text-right">
                                                                        <label class="">Kemaskini : </label>
                                                                        <span class="onoffswitch">
                                                                            <input type="checkbox" name="sah[]" class="onoffswitch-checkbox" id="mKakitangan-btn" value="off">
                                                                            <label class="onoffswitch-label" for="mKakitangan-btn">
                                                                                <span class="onoffswitch-inner" data-swchon-text="YES" data-swchoff-text="NO"></span>
                                                                                <span class="onoffswitch-switch"></span>
                                                                            </label>

                                                                        </span>
                                                                        <input type="hidden" name="kemaskini" class="form-control" value="off">
                                                                    </div>
                                                                </div>
                                                            }

                                                        </div>
                                                    </fieldset>

                                                </div>
                                            }
                                        </div>
                                        @Html.HiddenFor(model => model.HR_MAKLUMAT_PERIBADI.HR_NO_PEKERJA)
                                        @Html.HiddenFor(model => model.HR_MAKLUMAT_PERIBADI.HR_NAMA_PEKERJA)
                                        @Html.HiddenFor(model => model.HR_MAKLUMAT_PERIBADI.HR_NO_KPBARU)
                                    }
                                    <br />
                                    if (ViewBag.key == "4")
                                    {
                                        <div id="lihat-info">
                                            <div class="row">
                                                <div class="col-md-12 tab-pane">
                                                    <ul class="nav nav-tabs bordered">
                                                        <li class="active">
                                                            <a href="#Peribadi" data-toggle="tab">Peribadi</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Pekerjaan" data-toggle="tab">Pekerjaan</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Kemahiran" data-toggle="tab">Kemahiran</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Akademik" data-toggle="tab">Akademik</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Pewaris" data-toggle="tab">Pewaris</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Tanggungan" data-toggle="tab">Tanggungan</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Kuarters" data-toggle="tab">Kuarters</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Gaji" data-toggle="tab">Gaji/Elaun/Caruman/Potongan</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Anugerah" data-toggle="tab">Anugerah</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Persaraan" data-toggle="tab">Persaraan</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Tatatertib" data-toggle="tab">Tatatertib</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Kematian" data-toggle="tab">Kematian</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Prestasi" data-toggle="tab">Prestasi</a>
                                                        </li>
                                                        <li>
                                                            <a href="#Cuti" data-toggle="tab">Cuti</a>
                                                        </li>
                                                    </ul>
                                                    <div class="tab-content padding-10">
                                                        <div class="tab-pane active" id="Peribadi">
                                                            <div class="row">
                                                                <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                    <legend>
                                                                        Maklumat Pengguna
                                                                    </legend>
                                                                    <div class="row smart-form">

                                                                        <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                            <div class="row">
                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Status</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="select">
                                                                                            @Html.DropDownListFor(model => model.HR_MAKLUMAT_PERIBADI.HR_AKTIF_IND, new SelectList(ViewBag.HR_AKTIF_IND, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                            <i></i>
                                                                                        </label>
                                                                                    </div>

                                                                                </section>

                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Alasan</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="select">
                                                                                            @Html.DropDownListFor(model => model.HR_MAKLUMAT_PERIBADI.HR_ALASAN, new SelectList(ViewBag.HR_ALASAN, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                            <i></i>
                                                                                        </label>
                                                                                    </div>

                                                                                </section>
                                                                            </div>

                                                                            <div class="row">
                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No. KP (Lama)</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="input">
                                                                                            <i class="icon-append fa fa-list-alt"></i>
                                                                                            @Html.EditorFor(model => model.HR_MAKLUMAT_PERIBADI.HR_NO_KPLAMA, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                                                        </label>

                                                                                    </div>

                                                                                </section>

                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Taraf Perkahwinan</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="select">
                                                                                            @Html.DropDownListFor(model => model.HR_MAKLUMAT_PERIBADI.HR_TARAF_KAHWIN, new SelectList(ViewBag.TarafKahwin, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                            <i></i>
                                                                                        </label>
                                                                                    </div>

                                                                                </section>
                                                                            </div>

                                                                            <div class="row">
                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Tarikh Lahir</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="input">
                                                                                            <i class="icon-append fa fa-calendar"></i>
                                                                                            <input type="text" name="HR_MAKLUMAT_PERIBADI.HR_TARIKH_LAHIR" id="HR_MAKLUMAT_PERIBADI_HR_TARIKH_LAHIR" class="form-control" value="@string.Format("{0:dd/MM/yyyy}", Model.HR_MAKLUMAT_PERIBADI.HR_TARIKH_LAHIR)" disabled="disabled" />

                                                                                        </label>
                                                                                    </div>

                                                                                </section>

                                                                                <section class="col col-lg-6 col-md-6 col-sm-4 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Umur</label>
                                                                                    <div class="col-lg-2 col-md-2 col-sm-4 col-xs-4">
                                                                                        <label class="input">
                                                                                            <input type="text" name="Umur" id="Umur" class="form-control" value="@ViewBag.Umur" disabled="disabled" />
                                                                                        </label>
                                                                                    </div>

                                                                                </section>
                                                                            </div>

                                                                            <div class="row">
                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Tempat Lahir</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="select">
                                                                                            @Html.DropDownListFor(model => model.HR_MAKLUMAT_PERIBADI.HR_TEMPAT_LAHIR, new SelectList(ViewBag.TempatLahir, "Value", "Text"), htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                            <i></i>
                                                                                        </label>
                                                                                    </div>

                                                                                </section>

                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Kewarganegaraan</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="select">
                                                                                            @Html.DropDownListFor(model => model.HR_MAKLUMAT_PERIBADI.HR_WARGANEGARA, new SelectList(ViewBag.HR_WARGANEGARA, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                            <i></i>
                                                                                        </label>
                                                                                    </div>
                                                                                </section>
                                                                            </div>

                                                                            <div class="row">
                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Agama</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="select">
                                                                                            @Html.DropDownListFor(model => model.HR_MAKLUMAT_PERIBADI.HR_AGAMA, new SelectList(ViewBag.Agama, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                            <i></i>
                                                                                        </label>
                                                                                    </div>

                                                                                </section>

                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Keturunan</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="select">
                                                                                            @Html.DropDownListFor(model => model.HR_MAKLUMAT_PERIBADI.HR_KETURUNAN, new SelectList(ViewBag.Keturunan, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                            <i></i>
                                                                                        </label>
                                                                                    </div>

                                                                                </section>
                                                                            </div>

                                                                            <div class="row">
                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">E-mel</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="input">
                                                                                            <i class="icon-append fa fa-envelope-o"></i>
                                                                                            @Html.EditorFor(model => model.HR_MAKLUMAT_PERIBADI.HR_EMAIL, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                                                            <b class="tooltip tooltip-top-right">
                                                                                                <i class="fa fa-warning txt-color-teal"></i>
                                                                                                Sila isikan e-mel dengan betul (cth: = example@mbpj.gov.my)
                                                                                            </b>
                                                                                        </label>
                                                                                    </div>

                                                                                </section>

                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Jantina</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <div class="inline-group">
                                                                                            <label class="radio">
                                                                                                @Html.RadioButtonFor(model => model.HR_MAKLUMAT_PERIBADI.HR_JANTINA, "L", htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                                <i></i>Lelaki
                                                                                            </label>
                                                                                            <label class="radio">
                                                                                                @Html.RadioButtonFor(model => model.HR_MAKLUMAT_PERIBADI.HR_JANTINA, "P", htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                                <i></i>Perempuan
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>

                                                                                </section>

                                                                            </div>
                                                                            <div class="row">
                                                                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Gelaran</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="select">
                                                                                            @Html.DropDownListFor(model => model.HR_MAKLUMAT_PEKERJAAN.HR_GELARAN, new SelectList(ViewBag.HR_GELARAN, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                                                            <i></i>
                                                                                        </label>
                                                                                    </div>

                                                                                </section>
                                                                            </div>
                                                                        </div>

                                                                    </div>
                                                                </fieldset>
                                                                <br />
                                                                <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                    <legend>
                                                                        Alamat
                                                                    </legend>
                                                                    <div class="row">

                                                                        <div class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                                            <div class="row smart-form">
                                                                                <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Tetap</label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="input">
                                                                                            @Html.EditorFor(model => model.HR_MAKLUMAT_PERIBADI.HR_TALAMAT1, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                                                        </label>
                                                                                    </div>

                                                                                </section>


                                                                            </div>
                                                                            <div class="row smart-form">
                                                                                <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"></label>
                                                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                                                        <label class="input">
                                                                                            @Html.EditorFor(model => model.HR_MAKLUMAT_PERIBADI.HR_TALAMAT2, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                                                        </label>
                                                                                    </div>
                                                                                </section>
                                                                            </div>
                                                                            <div class="row smart-form">
                                                                                <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
