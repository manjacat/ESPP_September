                    $("#iss1 input[name='HR_TARIKH_LAHIR']").removeAttr('readonly', 'readonly');
                    $("#iss1 input[name='Umur']").removeAttr('readonly', 'readonly');
                    $("#iss1 input[type='text']").attr('disabled', 'disabled');
                    $("#iss1 select").attr('disabled', 'disabled');
