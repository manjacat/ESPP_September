

@model eSPP.Models.HR_TUNTUTAN_INSURAN
<div>
    <div class="modal-header">
        <button type="button" class="close" id="Tambah-btn" data-dismiss="modal" aria-hidden="true">
            &times;
        </button>
        <h4 class="modal-title">
            Penerimaan Pembayaran <span class="error"></span>
        </h4>
    </div>
    <div class="modal-body no-padding">

        @using (Html.BeginForm("TambahTuntutanPembayaran", "TuntutanInsuran", FormMethod.Post, new { id = "order-form", @class = "smart-form", novalidate = "novalidate" }))
            {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <div id="peribadi">
               
                    <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">

                        <div class="row">

                            <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Jenis Insuran</label>
                                        <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.EditorFor(model => model.HR_JENIS_TUNTUTAN, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                            </label>
                                        </div>
                                    </section>
                                </div>
                                <br />
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Tarikh Tuntutan</label>
                                        <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.EditorFor(model => model.HR_TARIKH_TUNTUTAN, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                            </label>
                                        </div>
                                    </section>
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No. Polisi</label>
                                        <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.EditorFor(model => model.HR_NO_POLISI, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>
                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> Tarikh Pembayaran</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.EditorFor(model => model.HR_TARIKH_PEMBAYARAN, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>
                                    </section>

                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Jumlah Tuntutan</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="select">
                                                @Html.DropDownListFor(model => model.HR_JUMLAH_TUNTUTAN, null, String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                <i></i>
                                            </label>
                                        </div>
                                    </section>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                <footer>
                    <button type="button" class="btn btn-default" onclick="javascript: history.go(0)">
                        Kembali
                    </button>
                </footer>
            </div>
          }



    </div>

</div>

<script type="text/javascript">
        $(document).ready(function () {
            $("#Tambah-btn").click(function () {
                debugger
                $('#TambahModal').modal('hide');
            });



        })
</script>
