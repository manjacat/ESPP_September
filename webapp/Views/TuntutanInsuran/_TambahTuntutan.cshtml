
@model eSPP.Models.HR_TUNTUTAN_INSURAN
<div>
    <div class="modal-header">
        <button type="button" class="close" id="Tambah-btn" data-dismiss="modal" aria-hidden="true">
            &times;
        </button>
        <h4 class="modal-title">
            Tambah Tuntutan Insuran <span class="error"></span>
        </h4>
    </div>
    <div class="modal-body no-padding">

        @using (Html.BeginForm("TambahTuntutan", "TuntutanInsuran", FormMethod.Post, new { id = "order-form", @class = "smart-form", novalidate = "novalidate" }))
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            @Html.HiddenFor(model => model.HR_NO_PEKERJA)
            @Html.HiddenFor(model => model.HR_JENIS_TUNTUTAN)
            @Html.HiddenFor(model => model.HR_TARIKH_TUNTUTAN)

            <div id="peribadi">

                @if (Model.HR_JENIS_TUNTUTAN == "C")
                {
                    <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <label class="label">
                            Peringatan : Bahagian yang bertanda (<font color="red">*</font>) adalah wajib di isi.
                        </label>

                        <div class="row">

                            <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="row">
                                    <section class="col col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No. Pekerja</label>
                                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.EditorFor(model => model.HR_NO_PEKERJA, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                            </label>
                                        </div>
                                    </section>
                                    <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Nama Pekerja</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.Editor("HR_NAMA_PEKERJA", (string)ViewBag.HR_NAMA_PEKERJA, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                            </label>
                                        </div>

                                    </section>
                                    <section class="col col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No. KP</label>
                                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.Editor("HR_NO_KPBARU", (string)ViewBag.HR_NO_KPBARU, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>
                                    </section>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <legend style="background-color:white">
                            1. Maklumat Pesakit dan Insuran
                        </legend>
                        <div class="row">
                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> 1. a) No. Polisi<font color="red">*</font></label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                    <label class="input">
                                        <i class="icon-append fa fa-list-alt"></i>
                                        @Html.EditorFor(model => model.HR_NO_POLISI, new { htmlAttributes = new { @class = "form-control" }, maxlength = "14" })
                                    </label>
                                </div>
                            </section>

                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">b) Pelan Kategori</label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                    <label class="input">

                                        @Html.EditorFor(model => model.HR_PELAN_KATEGORI, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })

                                    </label>
                                </div>
                            </section>
                        </div>
                        <div class="row">
                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">2. a) Tarikh Mula Bekerja</label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                    <label class="input">

                                        @Html.Editor("HR_TARIKH_MASUK", (string)ViewBag.HR_TARIKH_MASUK, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                    </label>
                                </div>
                            </section>
                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> b) Tarikh Lahir</label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                    <label class="input">

                                        @Html.Editor("HR_TARIKH_LAHIR_PERIBADI", (string)ViewBag.HR_TARIKH_LAHIR_PERIBADI, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                    </label>
                                </div>

                            </section>
                        </div>
                        <div class="row">
                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> 3.  a) Pesakit adalah</label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                    <label class="select">
                                        @Html.DropDownListFor(model => model.HR_PESAKIT_IND, new SelectList(ViewBag.pelan, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control" })
                                        <i></i>
                                    </label>
                                </div>

                            </section>

                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> b) Jantina</label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                    <div class="inline-group">
                                        <label class="radio">
                                            @Html.RadioButton("HR_JANTINA_PERIBADI", (string)ViewBag.HR_JANTINA_PERIBADI, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            <i></i>Perempuan
                                        </label>
                                        <label class="radio">
                                            @Html.RadioButton("HR_JANTINA_PERIBADI", (string)ViewBag.HR_JANTINA_PERIBADI, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            <i></i>Lelaki
                                        </label>
                                    </div>
                                </div>

                            </section>

                        </div>


                    </fieldset>
                    <br />
                    <div id="Tanggungan">
                        <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">

                            <label class="label">
                                Jika pesakit adalah tanggungan, sila berikan maklumat berikut :
                            </label>
                            <br />
                            <div class="row">

                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">  4.  Nama Pesakit</label>
                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                        <label class="select">
                                            @Html.DropDownList("HR_NAMA_TANGGUNGAN", null, String.Empty, new { @class = "form-control", disabled = "disabled" })
                                            <i></i>
                                        </label>
                                    </div>

                                </section>

                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">    No. KP Baru</label>
                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                        <label class="input">

                                            @Html.Editor("HR_NO_KP_PESAKIT", null, new { htmlAttributes = new { @class = "form-control"} })

                                        </label>
                                    </div>

                                </section>
                            </div>
                            <div class="row">
                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">  5.  Tarikh Lahir Pesakit</label>
                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                        <label class="input">

                                            @Html.Editor("HR_TARIKH_LAHIR", null, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })

                                        </label>
                                    </div>

                                </section>
                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> b) Jantina</label>
                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                        <div class="inline-group">
                                            <label class="radio">
                                                @Html.RadioButton("HR_JANTINA", "P", new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                <i></i>Perempuan
                                            </label>
                                            <label class="radio">
                                                @Html.RadioButton("HR_JANTINA", "L", new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                <i></i>Lelaki
                                            </label>
                                        </div>
                                    </div>

                                </section>


                            </div>
                        </fieldset>
                    </div>
                        <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <legend style="background-color:white">
                                2. Keterangan Mengenai Penyakit dan Kecederaan
                            </legend>

                            <label class="label">
                               <font color="red"> Sila isikan 1 atau 2 dan 3</font>
                            </label>
                            <br />
                            <section>
                                <div class="row">
                                    <div class="col col-lg-12">
                                        <label class="radio"><input type="radio" name="checkbox" value="1"><i></i>1</label>
                                        <div id="satu">
                                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> 1 a) Jenis Penyakit (atau keterangan jika nama penyakit tidak diketahui)</label>
                                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">

                                                    <label class="input">

                                                        <input type="text" name="HR_NAMA_PENYAKIT" id="HR_NAMA_PENYAKIT" class="form-control" disabled="disabled" />
                                                    </label>
                                                </div>

                                            </section>
                                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">   1 b) Tarikh mula simptom disedari oleh pesakit</label>
                                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                    <label class="input">
                                                        <i class="icon-append fa fa-calendar"></i>
                                                        <input type="text" name="HR_TARIKH_SIMPTOM" id="HR_TARIKH_SIMPTOM" class="form-control" value="@string.Format("{0:dd/MM/yyyy}", Model.HR_TARIKH_SIMPTOM)" disabled="disabled" />
                                                    </label>
                                                </div>

                                            </section>
                                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">  1 c) Tarikh nasihat pertama diterima dari pakar untuk penyakit ini</label>
                                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                    <label class="input">
                                                        <i class="icon-append fa fa-calendar"></i>
                                                        <input type="text" name="HR_TARIKH_NASIHAT" id="HR_TARIKH_NASIHAT" class="form-control" value="@string.Format("{0:dd/MM/yyyy}", Model.HR_TARIKH_NASIHAT)" disabled="disabled" />
                                                    </label>
                                                </div>
                                            </section>
                                        </div>
                                    </div>
                                </div>
                            </section>


                            <section>
                                <div class="row">
                                    <div class="col col-lg-12">
                                        <label class="radio"><input type="radio" name="checkbox" value="2"><i></i>2</label>
                                        <div id="dua">
                                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> 2 a) Terangkan kecederaan dan bagaimana ia berlaku</label>
                                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                    <label class="input">
                                                        @Html.EditorFor(model => model.HR_KECEDERAAN, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                    </label>
                                                </div>

                                            </section>
                                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> 2 b) Tarikh dan masa kecederaan berlaku</label>
                                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                    <label class="input">
                                                        <i class="icon-append fa fa-calendar"></i>
                                                        <input type="text" name="HR_TARIKH_MASA_CEDERA" id="HR_TARIKH_MASA_CEDERA" class="form-control" value="@string.Format("{0:dd/MM/yyyy}", Model.HR_TARIKH_MASA_CEDERA)" disabled="disabled" />
                                                    </label>
                                                </div>
                                            </section>

                                            <section>
                                                <label class="label col col-2"> 2 c) Adakah ia berkaitan dengan kerja</label>
                                                <div class="col col-3">
                                                    <div class="inline-group">
                                                        <label class="radio">
                                                            @Html.RadioButtonFor(model => model.HR_KERJA_IND, "Y", htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                            <i></i>Aktif
                                                        </label>
                                                        <label class="radio">
                                                            @Html.RadioButtonFor(model => model.HR_KERJA_IND, "T", htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                            <i></i>Tidak Aktif
                                                        </label>
                                                    </div>
                                                </div>
                                            </section>

                                        </div>
                                    </div>
                                </div>
                            </section>
                            <br />

                            <section>
                                <div class="col col-lg-12">

                                    <div id="tiga">
                                        <div class="row">
                                            <section>
                                                <label class="label col col-6">3 a) Adakah penyakit atau kecederaan pernah dirawat?</label>
                                                <div class="col col-lg-6">
                                                    <div class="inline-group">
                                                        <label class="radio" name="radio" value="Y">
                                                            @Html.RadioButtonFor(model => model.HR_RAWATAN_IND, "Y", htmlAttributes: new { @class = "form-control" })
                                                            <i></i>Aktif
                                                        </label>
                                                        <label class="radio">
                                                            @Html.RadioButtonFor(model => model.HR_RAWATAN_IND, "T", htmlAttributes: new { @class = "form-control" })
                                                            <i></i>Tidak Aktif
                                                        </label>
                                                    </div>
                                                </div>

                                            </section>
                                        </div>
                                        <div class="row">
                                            <section>
                                                <label class="label col col-6">3 b) Jika rawatan berkaitan dengan mengandung, keguguran, bersalin belum tempoh matang, kemandulan, sub-fertility atau infertility?</label>
                                                <div class="col col-lg-6">
                                                    <div class="inline-group">
                                                        <label class="radio" name="radio" value="Y">
                                                            @Html.RadioButtonFor(model => model.HR_KAITAN_RAWATAN_IND, "Y", htmlAttributes: new { @class = "form-control" })
                                                            <i></i>Aktif
                                                        </label>
                                                        <label class="radio">
                                                            @Html.RadioButtonFor(model => model.HR_KAITAN_RAWATAN_IND, "T", htmlAttributes: new { @class = "form-control" })
                                                            <i></i>Tidak Aktif
                                                        </label>
                                                    </div>
                                                </div>
                                            </section>
                                        </div>

                                        <br />
                                        <label class="label">
                                            Jika Ya bagi salah satu soalan, sila sertakan nama Doktor penasihat
                                        </label>
                                        <br />
                                        <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                            <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> Nama</label>
                                            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                <label class="input">

                                                    @Html.EditorFor(model => model.HR_NAMA_DOKTOR, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                </label>
                                            </div>

                                        </section>
                                        <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                            <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">  Alamat</label>
                                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                <label class="select">
                                                    @Html.DropDownListFor(model => model.HR_KOD_AGENSI_DOKTOR, new SelectList(ViewBag.kodAgensi, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                    <i></i>
                                                </label>
                                            </div>
                                        </section>
                                        <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                            <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">  Tarikh Mula Menerima Nasihat</label>
                                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                <label class="input">
                                                    <i class="icon-append fa fa-calendar"></i>
                                                    <input type="text" name="HR_TARIKH_RAWATAN" id="HR_TARIKH_RAWATAN" readonly='readonly' disabled="disabled">
                                                </label>
                                            </div>
                                        </section>

                                    </div>
                                </div>
                            </section>

                        </fieldset>
                        @*<fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <legend style="background-color:white">
                                3. Maklumat Lain
                            </legend>
                            <div class="row">
                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> 1. Nama Hospital/Klinik</label>
                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                        <label class="select">
                                            @Html.DropDownListFor(model => model.HR_KOD_AGENSI_RAWATAN, new SelectList(ViewBag.kodRawatan, "Value", "Text"), String.Empty, htmlAttributes: new { @class = "form-control" })
                                            <i></i>
                                        </label>
                                    </div>
                                </section>

                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">2. Tarikh Masuk</label>
                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                        <label class="input">
                                            <i class="icon-append fa fa-calendar"></i>
                                            <input type="text" name="HR_TARIKH_MASUK_HOSP" id="HR_TARIKH_MASUK_HOSP" readonly='readonly'>
                                        </label>
                                    </div>
                                </section>

                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">3. Tarikh Pembedahan</label>
                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                        <label class="input">
                                            <i class="icon-append fa fa-calendar"></i>
                                            <input type="text" name="HR_TARIKH_BEDAH" id="HR_TARIKH_BEDAH" readonly='readonly'>
                                        </label>
                                    </div>
                                </section>
                                <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                    <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">4. Tarikh Keluar</label>
                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                        <label class="input">
                                            <i class="icon-append fa fa-calendar"></i>
                                            <input type="text" name="HR_TARIKH_KELUAR_HOSP" id="HR_TARIKH_KELUAR_HOSP" readonly='readonly'>
                                        </label>
                                    </div>

                                </section>
                            </div>
                            <div class="row">
                                <section>
                                    <label class="label col col-6">5. Adakah anda dibenarkan membuat tuntutan rawatan ini dengan polisi insuran syarikat lain?</label>
                                    <div class="col col-lg-6">
                                        <div class="inline-group">
                                            <label class="radio" name="radio" value="Y">
                                                @Html.RadioButtonFor(model => model.HR_CLAIM_IND, "Y", htmlAttributes: new { @class = "form-control" })
                                                <i></i>Aktif
                                            </label>
                                            <label class="radio">
                                                @Html.RadioButtonFor(model => model.HR_CLAIM_IND, "T", htmlAttributes: new { @class = "form-control" })
                                                <i></i>Tidak Aktif
                                            </label>
                                        </div>
                                    </div>

                                </section>
                            </div>
                            <br />

                            <label class="label">
                                Jika Ya , sila sertakan nama syarikat insuran berkenaan
                            </label>
                            <br />
                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> Nama Syarikat</label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                    <label class="input">

                                        @Html.EditorFor(model => model.HR_SYKT_INSURANS, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                    </label>
                                </div>

                            </section>
                            <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-4">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">  Nombor Polisi</label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                    <label class="input">
                                        @Html.EditorFor(model => model.HR_NO_POLISI_LAIN, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                        <label class="label">
                                            (Sila sertakan salinan Polisi Insuran selain dari Workmen's compensation, jika perlu)
                                        </label>
                                    </label>
                                </div>
                            </section>


                            <label class="label">
                                6. Tuntutan perlu dibuat ke atas:-
                            </label>
                            <section class="col col-lg-6 col-md-6 col-sm-6 col- xs-6">
                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"> a) Hospital</label>
                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
