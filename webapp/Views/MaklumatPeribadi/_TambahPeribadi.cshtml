

@model eSPP.Models.HR_PENSYARAH
<div>
    <div class="modal-header">
        <button type="button" class="close" id="Tambah-btn" data-dismiss="modal" aria-hidden="true">
            &times;
        </button>
        <h4 class="modal-title">
            Tambah Kumpulan Pensyarah <span class="error"></span>
        </h4>
    </div>
    <div class="modal-body no-padding">
        
            @using (Html.BeginForm("TambahPeribadi", "MaklumatPeribadi", FormMethod.Post, new { id = "order-form", @class = "smart-form", novalidate = "novalidate" }))
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div id="peribadi">

                    <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">

                        <div class="row">

                            <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">ID. Pensyarah</label>
                                        <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.EditorFor(model => model.HR_NO_PENSYARAH, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                            </label>
                                        </div>
                                    </section>
                                </div>
                                <br />
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Kategori Pensyarah</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <div class="inline-group">
                                                <label class="radio">
                                                    @Html.RadioButtonFor(model => model.HR_KUMPULAN_PENSYARAH.HR_JENIS_IND, "D", htmlAttributes: new { @class = "form-control" })
                                                    <i></i>Pensyarah Dalam
                                                </label>
                                                <label class="radio">
                                                    @Html.RadioButtonFor(model => model.HR_KUMPULAN_PENSYARAH.HR_JENIS_IND, "L", htmlAttributes: new { @class = "form-control" })
                                                    <i></i>Pensyarah Luar
                                                </label>
                                            </div>
                                        </div>
                                    </section>
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No.Pekerja</label>
                                        <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.EditorFor(model => model.HR_NO_PEKERJA, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>
                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Nama Pensyarah</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                <i class="icon-append fa fa-list-alt"></i>
                                                @Html.EditorFor(model => model.HR_NAMA_PENSYARAH, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>
                                    </section>

                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Jenis Pensyarah</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="select">
                                                @Html.DropDownListFor(model => model.HR_KOD_KUMPULAN, null, String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                <i></i>
                                            </label>
                                        </div>
                                    </section>
                                </div>

                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No.KP Baru</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">

                                                @Html.EditorFor(model => model.HR_NO_KPBARU, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>

                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No.KP Lama</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">

                                                @Html.EditorFor(model => model.HR_NO_KPLAMA, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No.Telefon Pejabat</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">

                                                @Html.EditorFor(model => model.HR_NO_TELPEJABAT, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })

                                            </label>
                                        </div>

                                    </section>

                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No.Telefon Bimbit</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">

                                                @Html.EditorFor(model => model.HR_NO_TELBIMBIT, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">No. Fax</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">

                                                @Html.EditorFor(model => model.HR_NO_FAKS, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>

                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Jawatan</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">

                                                @Html.EditorFor(model => model.HR_JAWATAN, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })

                                            </label>
                                        </div>

                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Gred/Kelulusan</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">

                                                @Html.EditorFor(model => model.HR_GRED_KELULUSAN, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })

                                            </label>
                                        </div>

                                    </section>

                                    <section class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Gaji Pokok Sebulan RM</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">

                                                @Html.EditorFor(model => model.HR_GAJI_POKOK, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })

                                            </label>
                                        </div>

                                    </section>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <br />
                    <fieldset class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <legend style="background-color:white">
                            Alamat
                        </legend>
                        <div class="row">

                            <div class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Tetap</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                @Html.EditorFor(model => model.HR_TALAMAT1, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>

                                </div>
                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"></label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                @Html.EditorFor(model => model.HR_TALAMAT2, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>
                                    </section>
                                </div>
                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"></label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                @Html.EditorFor(model => model.HR_TALAMAT3, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>
                                </div>

                                <div class="row">
                                    <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <div class="row">
                                            <section class="col col-lg-7 col-md-7 col-sm-7 col-xs-7">
                                                <label class="label col-lg-7 col-md-7 col-sm-12 col-xs-12">Poskod</label>
                                                <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12">
                                                    <label class="input">
                                                        <i class="icon-append fa fa-question-circle"></i>
                                                        @Html.EditorFor(model => model.HR_TPOSKOD, new { htmlAttributes = new { @class = "form-control", disabled = "disabled", maxlength = 5 } })
                                                        <b class="tooltip tooltip-top-right">
                                                            <i class="fa fa-warning txt-color-teal"></i>
                                                            Sila isi hanya nombor sahaja
                                                        </b>
                                                    </label>
                                                </div>

                                            </section>

                                            <section class="col col-lg-5 col-md-5 col-sm-5 col-xs-5">
                                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Bandar</label>
                                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                    <label class="input">
                                                        @Html.EditorFor(model => model.HR_TBANDAR, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                    </label>
                                                </div>

                                            </section>
                                        </div>

                                    </div>

                                </div>

                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Negeri</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="select">
                                                @Html.DropDownListFor(model => model.HR_TNEGERI, null, String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                <i></i>
                                            </label>
                                        </div>

                                    </section>
                                </div>
                            </div>
                            <div class="col col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Surat - Menyurat</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                @Html.EditorFor(model => model.HR_SALAMAT1, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>

                                </div>
                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"></label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                @Html.EditorFor(model => model.HR_SALAMAT2, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>


                                </div>

                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"></label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <label class="input">
                                                @Html.EditorFor(model => model.HR_SALAMAT3, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                            </label>
                                        </div>

                                    </section>
                                </div>

                                <div class="row">
                                    <div class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <div class="row">
                                            <section class="col col-lg-7 col-md-7 col-sm-7 col-xs-7">
                                                <label class="label col-lg-7 col-md-7 col-sm-12 col-xs-12">Poskod</label>
                                                <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12">
                                                    <label class="input">
                                                        <i class="icon-append fa fa-question-circle"></i>
                                                        @Html.EditorFor(model => model.HR_SPOSKOD, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                        <b class="tooltip tooltip-top-right">
                                                            <i class="fa fa-warning txt-color-teal"></i>
                                                            Sila isikan nombor sahaja
                                                        </b>
                                                    </label>
                                                </div>

                                            </section>

                                            <section class="col col-lg-5 col-md-5 col-sm-5 col-xs-5">
                                                <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Bandar</label>
                                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                    <label class="input">
                                                        @Html.EditorFor(model => model.HR_SBANDAR, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                                    </label>
                                                </div>

                                            </section>
                                        </div>

                                    </div>

                                </div>

                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12">Negeri</label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">

                                            <label class="select">
                                                @Html.DropDownListFor(model => model.HR_SNEGERI, null, String.Empty, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                                                <i></i>
                                            </label>
                                        </div>

                                    </section>
                                </div>

                                <div class="row">
                                    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <label class="label col-lg-4 col-md-4 col-sm-12 col-xs-12"></label>
                                        <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                            <button type="button" id="btn-paste" name="btn-paste" class="btn btn-default" disabled="disabled" style="padding:5px 10px">
                                                <i class="fa fa-paste"> Ambil Data Dari Alamat Tetap</i>
                                            </button>
                                        </div>
                                    </section>
                                </div>
                            </div>

                        </div>

                    </fieldset>

                    <footer>
                        <button type="submit" id="btn" class="btn btn-primary">
                            <span class="fa fa-send-o"></span> Hantar
                        </button>
                        <button type="button" class="btn btn-default" onclick="javascript: history.go(0)">
                            Kembali
                        </button>
                    </footer>
                </div>
            }



    </div>

</div>

    <script type="text/javascript">
        $(document).ready(function () {
            $("#Tambah-btn").click(function () {
                debugger
                $('#TambahModal').modal('hide');
            });

            var $orderForm = $("#order-form").validate({
                // Rules for form validation
                rules: {
                    "HR_KUMPULAN_PENSYARAH.HR_JENIS_IND": {
                        required: true,
                        
                    },
                    HR_NO_PEKERJA: {
                        required: function (element) {
                            if ($("#HR_KUMPULAN_PENSYARAH_HR_JENIS_IND:checked").val() == "L")
                            {
                                return true;
                            }
                        },
                        remote: {
                            url: "MaklumatPeribadi/CariData",
                            type: "get",
                            data: {
                                DATA: function () {
                                    return $("#HR_NO_PEKERJA").val();
                                },
                                KATEGORI: function () {
                                    return $("#HR_KUMPULAN_PENSYARAH_HR_JENIS_IND:checked").val();
                                }
                            }
                            
                        }
                        
                    },
                    HR_NAMA_PENSYARAH: {
                        required: function (element) {
                            if ($("#HR_KUMPULAN_PENSYARAH_HR_JENIS_IND:checked").val() == "D") {
                                return true;
                            }
                        },
                        remote: {
                            url: "MaklumatPeribadi/CariData",
                            type: "get",
                            data: {
                                DATA: function () {
                                    return $("#HR_NAMA_PENSYARAH").val();
                                },
                                KATEGORI: function () {
                                    return $("#HR_KUMPULAN_PENSYARAH_HR_JENIS_IND:checked").val();
                                }
                            }
                            
                        }

                    },
                    HR_KOD_KUMPULAN: {
                        required: true,

                    }

                },

                // Messages for form validation
                messages: {
                    "HR_KUMPULAN_PENSYARAH.HR_JENIS_IND": {
                        required: 'Kategori pensyarah diperlukan'
                    },
                    HR_NO_PEKERJA: {
                        required: 'No pekerja diperlukan',

                    },
                    HR_NAMA_PENSYARAH: {
                        required: 'Nama pensyarah diperlukan',

                    },
                    HR_KOD_KUMPULAN: {
                        required: 'Jenis pensyarah diperlukan',

                    },
                },

                // Do not change code below
                errorPlacement: function (error, element) {
                    error.insertAfter(element.parent());
                }



            });


            $("#peribadi input[name='HR_KUMPULAN_PENSYARAH.HR_JENIS_IND']").on("click", function () {
                $("#peribadi input").not("#peribadi input[name='HR_KUMPULAN_PENSYARAH.HR_JENIS_IND']").val("");
                $("#peribadi select").val("");
                $("#btn-sub-tambah").removeAttr("disabled");
                $("#btn-sub-padam").removeAttr("disabled");
                $("#HR_KOD_KUMPULAN").removeAttr("disabled");
                if ($(this).val() == "D") {
                    $("#HR_NO_PEKERJA").removeAttr("disabled");
                    $("#peribadi input").not("#HR_NO_PENSYARAH, #HR_KUMPULAN_PENSYARAH_HR_JENIS_IND, #HR_NO_PEKERJA").attr("disabled", "disabled");
                    $("#btn-paste").attr("disabled", "disabled");
                    $("#HR_TNEGERI").attr("disabled", "disabled");
                    $("#HR_SNEGERI").attr("disabled", "disabled");
                }
                else {
                    $("#HR_NO_PEKERJA").attr("disabled", "disabled");
                    $("#peribadi input").not("#HR_NO_PENSYARAH, #HR_NO_PEKERJA").removeAttr("disabled");
                    $("#btn-paste").removeAttr("disabled");
                    $("#HR_TNEGERI").removeAttr("disabled");
                    $("#HR_SNEGERI").removeAttr("disabled");
                }
                $("#HR_KOD_KUMPULAN").html("<option></option>")
                $.ajax({
                    url: "MaklumatPeribadi/JenisPensyarah",
                    type: "GET",
                    data: { "HR_JENIS_IND": $(this).val() },
                    dataType: "Json",
                    success: function (data) {
                        $.each(data, function (i, v) {
                            $("#HR_KOD_KUMPULAN").append($("<option></option>").val(v.HR_KOD_KUMPULAN).html(v.HR_PENERANGAN))
                        })
                    }
                })

            });

            $("#HR_NO_PEKERJA, #HR_NAMA_PENSYARAH").on("input", function () {
                debugger
                $("#peribadi input").not("#" + $(this).attr('id') + ",#peribadi input[name='HR_KUMPULAN_PENSYARAH.HR_JENIS_IND']").val("");
                $("select").val("");
                var $this = $(this);
                $.ajax({
                    url: "MaklumatPeribadi/CariPensyarah",
                    type: "GET",
                    data: { "DATA": $(this).val(), "KATEGORI": $("#peribadi input[name='HR_KUMPULAN_PENSYARAH.HR_JENIS_IND']:checked").val() },
                    dataType: "Json",
                    success: function (data) {
                        if (data != true)
                        {
                            $("#HR_NO_PENSYARAH").val(data.HR_PENSYARAH.HR_NO_PENSYARAH);
                            //$("#HR_KUMPULAN_PENSYARAH_HR_JENIS_IND").val(data.HR_PENSYARAH.HR_KUMPULAN_PENSYARAH.HR_JENIS_IND);

                            //$("#HR_NO_PEKERJA").val(data.HR_NO_PEKERJA);
                            if ($this.attr("id") == "HR_NO_PEKERJA") {
                                $("#HR_NAMA_PENSYARAH").val(data.HR_PENSYARAH.HR_NAMA_PENSYARAH);
                            }

                            $("#HR_KOD_KUMPULAN").val(data.HR_PENSYARAH.HR_KOD_KUMPULAN);
                            $("#HR_NO_KPBARU").val(data.HR_PENSYARAH.HR_NO_KPBARU);
                            $("#HR_NO_KPLAMA").val(data.HR_PENSYARAH.HR_NO_KPLAMA);
                            $("#HR_NO_TELPEJABAT").val(data.HR_PENSYARAH.HR_NO_TELPEJABAT);

                            $("#HR_NO_TELBIMBIT").val(data.HR_PENSYARAH.HR_NO_TELBIMBIT);
                            $("#HR_NO_FAKS").val(data.HR_PENSYARAH.HR_NO_FAKS);
                            $("#HR_JAWATAN").val(data.HR_PENSYARAH.HR_JAWATAN);
                            $("#HR_GRED_KELULUSAN").val(data.HR_PENSYARAH.HR_GRED_KELULUSAN);
                            $("#HR_GAJI_POKOK").val(data.HR_PENSYARAH.HR_GAJI_POKOK);
